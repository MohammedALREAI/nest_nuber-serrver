{
  "name": "nuber-eat",
  "version": "0.0.1",
  "description": "nest with graphql and typorm ",
  "author": "<mhamad.aa1997.aa@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=prod nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "heroku-postbuild": "yarn --only=dev --no-shrinkwrap && yarn build && yarn gen"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "apollo-server-express": "^2.19.0",
    "aws-sdk": "^2.814.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "form-data": "^3.0.0",
    "got": "^11.8.1",
    "graphql": "^15.4.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^7.0.2",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mapped-types": "^0.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/mailgun-js": "^0.22.11",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.3.6",
    "jest": "26.4.2",
    "lint-staged": "^10.5.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0",
    "webpack-loader-helper": "^1.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      " yarn lint",
      "prettier --write"
    ],
    "{src,tests}/**/*.js": [
      "prettier --write"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/MohammedALREAI/nest_nuber-serrver.git"
}
